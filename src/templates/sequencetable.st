<table>
  $sequence:{<tr>
    <td>$it.("0")$</td>
    <td class="number" onmouseover="this.className='number hover';return true;" onmouseout="this.className='number';return true;">
      <span class="hotspot">$length(it.("1"))$</span>
      <div class="details">
        <strong>$it.("0")$</strong>
        $details(results=it.("1"))$
      </div>
    </td>
  </tr>}$
</table>

